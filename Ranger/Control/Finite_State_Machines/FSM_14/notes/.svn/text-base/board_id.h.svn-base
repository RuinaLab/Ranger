#ifndef __H_BOARD_ID__
#define __H_BOARD_ID__ 

typedef enum boards{
	BOARD_PC = 0,	//	Computer/Labview
	BOARD_MB = 1,	//	Main Brain
	BOARD_CSR = 2,	//	CAN/SSP Router
	BOARD_MCH = 3,	//	Hip Motor Controller
	BOARD_MCFO = 4,	//	Outer Foot Motor Controller
	BOARD_MCFI = 5,	//	Inner Foot Motor Controller
	BOARD_MCSO = 6,	//	Outer (old) Steering Motor Controller
	BOARD_MCSI = 7,	//	Inner (new) Steering Motor Controller
	BOARD_UI = 8,	//	User Interface
	BOARD_COL = 9,	//	Color sensor board
	BOARD_LAST
} BOARD_ID;


#endif //__H_BOARD_ID__

